{
    "manifest_version": 2,

    "name": "V065 2",
    "version": "1.0",
    "description": "Building a Image OCR extension",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icon48.png",
        "default_title": "image ocr"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": [
        "activeTab", "storage", "tabs", "downloads", "tabCapture", "pageCapture", "http://localhost/*"
    ]
}